local Material = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/MaterialLua/master/Module.lua"))()

local X = Material.Load({
	Title = "Find The Anime by Sumidasso",
	Style = 1,
	SizeX = 400,
	SizeY = 350,
	Theme = "Aqua",
	ColorOverrides = {
		MainFrame = Color3.fromRGB(235,235,235)
	}
})

local Page = X.New({
	Title = "All"
})

Page.Button({
	Text = "Naruto",
	Callback = function()
		loadstring(game:HttpGet('https://pastebin.com/raw/RwczxZij'))()
	end,
})

Page.Button({
	Text = "One Piece",
	Callback = function()
		loadstring(game:HttpGet('https://pastebin.com/raw/ULuN5TQx'))()
	end,
})

Page.Button({
	Text = "Dragon Ball",
	Callback = function()
		loadstring(game:HttpGet('https://pastebin.com/raw/HqWTLk2A'))()
	end,
})
